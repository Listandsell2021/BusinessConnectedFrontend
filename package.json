{
  "name": "leadform-server",
  "version": "1.0.0",
  "description": "Backend API for Leadform CRM",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "jest",
    "test:watch": "jest --watch",
